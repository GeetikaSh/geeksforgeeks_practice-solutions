class Solution:
    def longestUniqueSubsttr(self, S):
        # code here
        S1=" "
        i=0
        res = ''.join(sorted(S))
        
        for x in res:
            if res.index(x)==i:
                S1+=x
            i=i+1
        S1=S1.strip()
        return(len(S1))
 if __name__== '__main__'
    t=int(input())
    for_ in range(t):
       s=input().strip()
       
       
       ob=Solution()
       print(ob.longestUniqueSubsttr(s))
